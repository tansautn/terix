{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../out"
  },
  "package": {
    "productName": "next-tauri-app",
    "version": "0.1.0"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "identifier": "com.next-tauri-app.dev",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Next Tauri App",
        "width": 800,
        "height": 600
      }
    ]
  },
  "plugins": {
      "log": {
          "attachConsole": true
      }
  }
}